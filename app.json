{
  "expo": {
    "name": "cinetwork",
    "slug": "cinetwork",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "android",
      "ios"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.cinetwork.cinetwork"
    }
  }
}
